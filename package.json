{
  "name": "@pollinations/ipfs",
  "version": "0.1.82",
  "description": "pollinations ipfs state, messaging and storage",
  "type": "module",
  "exports": {
    "./*": "./src/*",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "npm run esbuild",
    "test": "echo \"Error: no test specified\" && exit 1",
    "predeploy": "npm run build",
    "install_backend": "cp -v dist/*-cli.js /usr/local/bin && cp -v scripts/*.sh /usr/local/bin && chmod ugoa+x /usr/local/bin/*.js",
    "esbuild": "esbuild src/*-cli.js --bundle --outdir=dist --platform=node --target=node14 --external:fsevents --external:electron"
  },
  "bin": {
    "pollinate": "dist/pollinate-cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pollinations/pollinations-ipfs.git"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.186.0",
    "@cto.af/textdecoder": "0.0.0",
    "@supabase/supabase-js": "^1.35.6",
    "async": "^3.2.1",
    "await-sleep": "^0.0.1",
    "buffer": "^6.0.3",
    "callback-to-async-iterator": "^1.1.1",
    "child-process-promise": "^2.2.1",
    "chokidar": "^3.5.3",
    "commander": "^7.2.0",
    "debounce": "^1.2.1",
    "debug": "^4.3.2",
    "event-iterator": "^2.0.0",
    "fs-extra": "^10.0.0",
    "ipfs-http-client": "^56.0.3",
    "ipfs-unixfs-exporter": "^8.0.6",
    "ipfs-unixfs-importer": "https://gitpkg.now.sh/pollinations/js-ipfs-unixfs/packages/ipfs-unixfs-importer?master",
    "is-ipfs": "^6.0.2",
    "is-port-reachable": "^3.0.0",
    "it-all": "^1.0.5",
    "it-concat": "^2.0.0",
    "it-first": "^1.0.6",
    "it-last": "^1.0.5",
    "it-reduce": "^1.0.5",
    "iter-tools": "^7.1.3",
    "json5": "^2.2.0",
    "lodash": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "markdown-to-text": "^0.1.1",
    "multicodec": "^3.1.0",
    "multiformats": "^9.7.0",
    "mz": "^2.7.0",
    "native-abort-controller": "^1.0.4",
    "node-abort-controller": "^3.0.1",
    "node-fetch": "^2.6.7",
    "p-queue": "^7.3.0",
    "path-browserify": "^1.0.1",
    "progress-stream": "^2.0.0",
    "promise-timeout": "^1.3.0",
    "queueable": "^4.1.4",
    "ramda": "^0.27.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "readline-async-generator": "^1.0.0",
    "sass": "^1.42.0",
    "social-post-api": "^1.0.20",
    "throttle-debounce": "^3.0.1",
    "tree-kill": "^1.2.2",
    "web3.storage": "^4.4.0",
    "webhook-discord": "^3.7.8"
  },
